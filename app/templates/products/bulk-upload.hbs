 <div class="container-fluid">
  <div class="row">

    <div class="col-xs-12 col-sm-12 col-lg-6 in-block nopadding">
      <div class="col-xs-12 nopadding bottom-spacer-container">
        <div class="form-group">
          <label for="InputEmail" class="upload-label full-width">Get to know how to upload</label>
          <div class="col-xs-12 col-lg-4 col-md-6 col-sm-6 nopaddingleft">
           <a href="http://www.pdf995.com/samples/pdf.pdf" download="test" target="_blank">
            <span class="btn btn-size-small btn-csv" style=""> 
              <i class="fa fa-file-excel-o"></i>download csv</span>
            </a>
            </div>
            <div class="col-xs-12 col-lg-4 col-md-6 col-sm-6 nopaddingleft">
              <span class="btn btn-size-small btn-csv">
                <i class="fa fa-file-pdf-o"></i>download guide</span>
              </div>
            </div>
          </div>
          <div class="col-xs-12 col-sm-12 col-lg-12 in-block nopadding">
            <div class="col-xs-12 nopadding">
              <div class="form-group">
                <label for="InputEmail"class="upload-label full-width">Please upload your product file</label>
                <div class="col-xs-12 nopaddingleft">
                 <form {{ action "upload" on="submit" }}>
                  <input type="hidden" name="MAX_FILE_SIZE" value="1000" />
                  <input type="file" id="uploadedFile" class="custom-file-input" />
                  <br />
                  <button type="submit" class="btn btn-primary btn-size-small paddtb10 col-xs-12 col-sm-12 col-md-2 col-lg-2">Upload</button>
                </form>
              </div>
            </div>
          </div>
        </div>

      </div>
      <div class="col-lg-6 col-md-6 col-xs-12 col-sm-12 upload-container in-block">
      </div>
    </div>
  </div>
