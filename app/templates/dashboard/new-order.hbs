<div class="col-lg-6 col-sm-12 col-md-12 nopaddingleft wrapper-outer">
  <div class="col-lg-12 col-md-12 col-sm-12 spacer20 dash-bar">
    <label for="exampleInputEmail1" class="in-block">New Order</label>
    <div class="col-lg-6 col-xs-2 col-md-6 col-sm-4 nopadding pull-right">
      <a href="profilpage.html">
        <div class="btn btn-dash btn-size-small pull-right nomargin">Details</div>
      </a>
    </div>
  </div>
  <div class="col-lg-12 col-md-12 col-sm-12 bordering spacer20 bottom-spacer-container in-block full-width">
    <div class="col-lg-6 col-xs-6 col-sm-6 nopadding in-block pull-left text-left bottom-spacer" style="display:none;">
      <i class="fa fa-certificate fa-colored"></i> &nbsp; <span class="order-notif">1 new order</span>
    </div>
    <div class="col-lg-12 col-xs-12 col-sm-12 nopadding in-block full-width">
      <div class="table-responsive table-scroll bottom-spacer">
        <table class="table table-striped table-responsive">
          <thead>
            <tr>
              <th>Date</th>
              <th>Order Number</th>
              <th>Customer</th>
            </tr>
          </thead>
          <tbody>
            {{#if isPending}}
              <p>Loading...</p>
            {{else}}
                {{#each order in orders}}
                <tr>
                  <td>{{ order.created_at }}</td>
                  <td>{{ order.order_number }}</td>
                  <td>{{ order.customer_name }}</td>
                </tr>
                {{/each}}
            {{/if}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
