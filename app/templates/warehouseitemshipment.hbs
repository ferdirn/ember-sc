
<form {{action "save" on="submit"}}  enctype="multipart/form-data">
 <div class="container-fluid">
  <div class="row">

    <div class="col-xs-12 col-sm-12 col-lg-4 col-md-4 in-block nopadding">
      <div class="col-xs-12 nopadding bottom-spacer-container">
        <div class="form-group">
		  <label for="sku" class="text-title">Order Number </label>
			  {{#auto-complete options=order_ids inputClass="foobar" selectedValue=model.order_number placeHolderText="Order Number" noMesssagePlaceHolderText="No order number found" selectItem="itemSelected"  as |result|}}
			    <p>{{result.order_number}}</p>
				{{/auto-complete}}

        </div>
      </div>

      <div class="col-xs-12 col-sm-12 col-lg-12 in-block nopadding">
        <div class="col-xs-12 nopadding">
          <div class="form-group">
            <label for="InputEmail"class="upload-label full-width">Please upload delivery receipt</label>
            <div class="col-lg-12 bottom-spacer">
              <div class="col-lg-2 col-xs-4 col-sm-3 nopadding in-block right-spacer">
              
                <img id="photo" src="{{model.photo}}" width="100%">
              </div>

              <div class="btn-photo-upload fileUpload btn btn-size-small pull-left">
                <span>Choose delivery scan</span>
                <input type="file" id="photo-file" class="upload" {{action "setPicture" on="change"}}>
              </div>

            </div>
          </div>
        </div>
      </div>
	
	<div class="col-xs-12 col-sm-12 col-lg-12 in-block nopadding">
      <div class="col-xs-12 nopadding">
		<div class="form-group">
		  <div class="col-sm-offset-3 col-sm-3 col-lg-3 pull-right">
			<button type="submit" class="btn btn-primary btn-size-small pull-right" >Save</button>
		  </div>
		</div>
	   </div>
	</div>

      <div>&nbsp;</div>


      </div>
      
    </div>
  </div>
  </form>
